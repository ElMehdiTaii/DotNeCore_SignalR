
@{
    ViewData["Title"] = "Dashboard";
}

<!-- CSS only -->
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

<div id="notification" class="badge badge-pill bg-primary" onclick="IsRead()"></div>
<div class="card">
    <div class="card-body">
        <div class="form-group">
            <label>Message Object</label>
            <input type="text" class="form-control" id="messageObject" />
        </div>
        <div class="form-group">
            <label>Message Content</label>
            <input type="text" class="form-control" id="messageContent" />
        </div>
        <input type="button" value="TestingData" class="btn btn-primary mt-2" onclick="addDataTest()" />
    </div>
</div>
<div class="card mt-3">
    <div class="card-body">
        <div class="row" style="height:100% auto; width:100% auto;">
            <div class="col-6">
                <ul id="messagesList"></ul>
            </div>
        </div>
    </div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

<script src="~/lib/microsoft/signalr/dist/browser/signalr.js"></script>

<script src="~/home.js"></script>



